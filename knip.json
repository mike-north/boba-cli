{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    ".": {
      "ignoreBinaries": ["changeset-change-detector"],
      "ignoreDependencies": ["@vitest/coverage-v8", "@commitlint/types"],
      "paths": {
        "@": ["src/index.ts"],
        "@/*": ["src/*"]
      }
    },
    "packages/*": {
      "entry": ["src/index.ts"],
      "paths": {
        "@": ["src/index.ts"],
        "@/*": ["src/*"]
      }
    }
  }
}

