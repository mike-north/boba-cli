{
  "$schema": "https://nx.dev/nx.schema.json",
  "name": "suds-tea",
  "sourceRoot": "packages/tea/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{projectRoot}/dist"
      ],
      "options": {
        "command": "pnpm -C packages/tea run build"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm -C packages/tea run test"
      }
    },
    "generate:api-report": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "{workspaceRoot}/api-reports/tea.api.md",
        "{workspaceRoot}/temp/tea.api.json"
      ],
      "options": {
        "command": "pnpm -C packages/tea run generate:api-report"
      }
    },
    "check:eslint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm -C packages/tea run lint"
      }
    },
    "check:api-report": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "generate:api-report"
      ],
      "outputs": [
        "{workspaceRoot}/api-reports/tea.api.md",
        "{workspaceRoot}/temp/tea.api.json"
      ],
      "options": {
        "command": "pnpm -C packages/tea run generate:api-report"
      }
    }
  },
  "tags": []
}

