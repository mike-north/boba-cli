{
  "name": "@suds-cli/tea",
  "version": "0.0.0",
  "description": "Bubble Tea-inspired runtime for Suds terminal UIs",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
