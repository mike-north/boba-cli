{
  "name": "@suds-cli/filesystem",
  "version": "0.0.0",
  "description": "Filesystem utility functions for Suds terminal UIs",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm run clean && tsc -p ./tsconfig.json",
    "test": "vitest run",
    "generate:api-report": "api-extractor run --local",
    "check:api-report": "pnpm run generate:api-report",
    "check:eslint": "pnpm run lint",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/unzipper": "^0.10.10",
    "typescript": "5.8.2",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
